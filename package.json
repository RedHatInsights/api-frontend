{
  "name": "insights-frontend-starter-app",
  "version": "1.1.0",
  "private": false,
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@patternfly/react-core": "4.214.1",
    "@patternfly/react-table": "^4.83.1",
    "@redhat-cloud-services/frontend-components": "^3.8.12",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.5",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.16",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "classnames": "^2.3.1",
    "js-file-download": "^0.4.12",
    "js-yaml": "^4.1.0",
    "jszip": "^3.9.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-json-view": "^1.21.3",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "swagger-ui-react": "^4.11.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "1.2.3",
    "@redhat-cloud-services/frontend-components-config": "^4.6.13",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.0",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "cross-fetch": "^3.1.5",
    "css-loader": "^6.7.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.15.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "stylelint": "^14.8.2",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-scss": "^4.2.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "scripts": {
    "build": "fec build",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "fec dev",
    "start:proxy": "PROXY=true npm run start",
    "start:proxy:beta": "BETA=true npm run start:proxy",
    "build:prod": "NODE_ENV=production fec build",
    "deploy": "npm-run-all build:prod lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy"
  },
  "insights": {
    "appname": "api-docs"
  }
}
